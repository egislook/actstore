{
  "name": "actstore",
  "version": "0.0.31",
  "description": "Just global store solution using react hooks, context and fetchier",
  "main": "out/index.js",
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "files": [
    "cli",
    "store",
    "utils"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "babel --watch src/ --out-dir ./out",
    "watch": "npm start & nodemon --delay 1.5 --watch /src/* --exec 'npm run rename'",
    "rename": "npm pack && mv actstore-0.*.*.tgz actstore.tgz",
    "lint": "prettier --tab-width 2 --jsx-bracket-same-line true --single-quote --print-width 100 true --semi false --write \"src/**/*.js\""
  },
  "homepage": "https://github.com/egislook/actstore#readme",
  "repository": "github:egislook/actstore",
  "bugs": {
    "url": "https://github.com/egislook/actstore/issues"
  },
  "keywords": [
    "react",
    "fetchier",
    "actstore"
  ],
  "author": "Team 7",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2"
  }
}
